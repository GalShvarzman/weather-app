{
  "manifest_version": 2,

  "name": "current temperature",
  "description": "This extension show current temperature",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://www.google.com/"
  ]
}
